<!doctype html>
<html lang="en">
@Html.Action("PartialHead", "Default")
<body id="home">
    <section class=" w3l-header-4 header-sticky">
        <!--header-->
        @Html.Action("PartialNavbar", "Default")
        <!--/header-->
    </section>
    @Html.Action("PartialSliderScript", "Default")
    <!--theme switcher dark and light mode script//-->
    <!-- breadcrumbs -->
    <section class="w3l-inner-banner-main">
        <div class="about-inner about editContent">
            <div class="container">
                <div class="breadcrumbs-sub">
                    <ul class="breadcrumbs-custom-path">
                        <li class="right-side propClone"><a href="~/Default/Index" class="editContent">Ana Sayfa <span class="fa fa-angle-right" aria-hidden="true"></span></a> <p></li>
                        <li class="active editContent">İletişim</li>
                    </ul>
                </div>
            </div>

        </div>
    </section>
    <!-- breadcrumbs //-->

    <section class="w3l-contact-info-main" id="contact">
        <div class="contact-sec	editContent">
            <div class="container">
                <div class="map-content-9 mb-5 ">
                    <form method="post">
                        <div class="twice-two">
                            <input type="text" class="form-control" name="NameSurname" id="w3lName" placeholder="Ad Soyad" required="">
                            <input type="email" class="form-control" name="Mail" id="w3lSender" placeholder="Mail Adresiniz" required="">
                            <input type="text" class="form-control" name="Subject" id="w3lSubject" placeholder="Konu" required="">
                        </div>
                        <textarea name="Message" class="form-control" id="w3lMessage" placeholder="Mesaj" required=""></textarea>

                        <div class="text-right">
                            <button type="submit" class="btn btn-contact">Mesaj Gönder</button>
                        </div>
                    </form>
                </div>

                @Html.Action("PartialContactUs")

            </div>
        </div>
    </section>


    
    <section class="w3l-footer-29-main">
        <div class="footer-29 py-5 ">
            <div class="container">
                <div class="grid-col-4 footer-top-29">
                    <div class="footer-list-29 footer-1">
                        @Html.Action("PartialFooter", "Default")

                        @Html.Action("PartialSocialMedia", "Default")
                    </div>
                    @Html.Action("PartialCategory", "Default")
                    @Html.Action("PartialGalery", "Default")
                    @Html.Action("PartialTravelTrip", "Default")
                </div>
                <div class=" bottom-copies text-center">
                    <p class="copy-footer-29">© 2020 Go Trip. All rights reserved | Designed by <a href="//w3layouts.com">W3layouts</a></p>
                </div>
            </div>
        </div>
    </section>
    @Html.Action("PartialMainScript", "Default")

</body>

</html>